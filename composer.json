{
    "name": "yiisoft/yii-queue-driver-synchronous",
    "type": "library",
    "description": "Driver for the yiisoft/yii-queue package which executes all the jobs synchronously.",
    "keywords": [
        "queue"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-queue-driver-synchronous/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-queue-driver-synchronous"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "yiisoft/yii-queue": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^3.0",
        "infection/infection": "^0.15.3",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Queue\\Driver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Queue\\Driver\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
